<div class="container">
  <h2>Users nearby:</h2>
  <div class="cards cards-users">
    <% @users.each do |user| %>
      <div class="card-user">
        <%= image_tag "https://media-exp1.licdn.com/dms/image/C5603AQEQl75flDaIFQ/profile-displayphoto-shrink_200_200/0/1517531922211?e=1634774400&v=beta&t=n2pCZbt7uHRkbNqQNwwjKcI69F9kBFp1dHtx9t1CPKs", class: "avatar-card"%>
        <h4><%= user.username %></h4>
      </div>
    <% end %>
  </div>
</div>
